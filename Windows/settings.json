{
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
  "theme": "dark",
  "initialCols": 125,
  "initialRows": 35,
  "profiles": {
    "defaults": {
      // Put settings here that you want to apply to all profiles
      "acrylicOpacity": 0.95,
      // "acrylicOpacity": 0.75,
      "useAcrylic": true,
      "colorScheme": "Subliminal",
      "cursorShape": "bar",
      "padding": "8, 8, 8, 8",
